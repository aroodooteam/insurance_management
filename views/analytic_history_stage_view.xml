<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="view_analytic_history_stage_tree" model="ir.ui.view">
            <field name="name">analytic.history.stage.tree</field>
            <field name="model">analytic.history.stage</field>
            <field name="type">tree</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Emission type">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>

        <record id="view_analytic_history_stage_form" model="ir.ui.view">
            <field name="name">analytic.history.stage.form</field>
            <field name="model">analytic.history.stage</field>
            <field name="type">form</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <form string="Emission type">
                <header>
                </header>
                <sheet>
                    <group colspan="12">
                        <group cols="4">
                            <field name="name"/>
                        </group>
                        <group cols="4">
                            <field name="code"/>
                        </group>
                    </group>
                </sheet>
            </form>
            </field>
        </record>

        <record id="view_analytic_history_stage_search" model="ir.ui.view">
            <field name="name">analytic.history.stage.search</field>
            <field name="model">analytic.history.stage</field>
            <field name="type">search</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
            <search string="Emission type">
                <field name="name" select="True"/>
            </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_analytic_history_stage_view">
            <field name="name">Emission type</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">analytic.history.stage</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_analytic_history_stage_search"/>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_analytic_history_stage_view_form">
            <field name="act_window_id" ref="act_open_analytic_history_stage_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_analytic_history_stage_form"/>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_analytic_history_stage_view_tree">
            <field name="act_window_id" ref="act_open_analytic_history_stage_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_analytic_history_stage_tree"/>
        </record>

        <menuitem id="menu_analytic_history_stage"
            parent="insurance_management.menu_root_configuration_insurance"
            sequence="80"
            action="act_open_analytic_history_stage_view"/>
    </data>
</openerp>
